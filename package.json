{
  "name": "hiq-charts-react",
  "sideEffects": false,
  "version": "0.0.6",
  "description": "React specific wrapper for hiq-charts",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf dist",
    "compile": "npm run tsc",
    "tsc": "tsc -p .",
    "rollup": "rollup -c"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@types/jest": "23.3.9",
    "@types/node": "^14.14.35",
    "@types/react": "16.7.6",
    "@types/react-dom": "16.0.9",
    "jest": "^26.6.3",
    "jest-dom": "^3.0.2",
    "np": "^7.4.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "typescript": "^3.3.4000"
  },
  "dependencies": {
    "hiq-charts": "0.0.4"
  },
  "peerDependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "jest": {
    "preset": "ts-jest",
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HolonIQ/hiq-charts-react.git"
  },
  "keywords": [
    "react",
    "HolonIQ",
    "charts",
    "visualization"
  ],
  "author": "Charlie@HolonIQ",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/HolonIQ/hiq-charts-react/issues"
  },
  "homepage": "https://github.com/HolonIQ/hiq-charts-react#readme"
}
